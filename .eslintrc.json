{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "jest": true
  },
  "globals": {
    "jest": false
  },
  "rules": {
    "no-mixed-operators": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "no-underscore-dangle": 0,
    "react/require-default-props": 0,
    "lines-between-class-members": 0,
    "react/destructuring-assignment": 0
  }
}
